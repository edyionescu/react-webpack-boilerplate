{
  "name": "react-webpack-boilerplate",
  "version": "0.1.0",
  "description": "React Webpack Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.production.js",
    "dev": "webpack-dev-server --open --config webpack.config.development.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "eslint ./"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "react",
    "webpack",
    "babel",
    "jest",
    "prettier",
    "eslint"
  ],
  "author": "Edy Ionescu <errorsmith@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/errorsmith/react-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/errorsmith/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/errorsmith/react-webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format -- --staged && npm run lint && npm test"
    }
  }
}
