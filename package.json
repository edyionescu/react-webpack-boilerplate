{
  "name": "react-webpack-boilerplate",
  "version": "0.1.0",
  "description": "React Webpack Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.production.js",
    "dev": "webpack-dev-server --open --config webpack.config.development.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "eslint ./"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "react",
    "webpack",
    "babel",
    "jest",
    "prettier",
    "eslint"
  ],
  "author": "Edy Ionescu <errorsmith@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/errorsmith/react-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/errorsmith/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/errorsmith/react-webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-dom": "^3.0.1",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "react-testing-library": "^5.4.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format -- --staged && npm run lint && npm test"
    }
  }
}
