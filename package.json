{
  "name": "react-webpack-boilerplate",
  "version": "0.1.0",
  "description": "React Webpack Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.production.js",
    "dev": "webpack-dev-server --open --config webpack.config.development.js",
    "dev:hot": "npm run dev -- --hot",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "eslint ./"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "react",
    "webpack",
    "babel",
    "jest",
    "prettier",
    "eslint"
  ],
  "author": "Edy Ionescu <errorsmith@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/errorsmith/react-webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/errorsmith/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/errorsmith/react-webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format -- --staged && npm run lint && npm test"
    }
  }
}
